{
    "defaultSeverity": "error",
    "extends": [
      "tslint-config-standard",
      "tslint-config-airbnb",
      "tslint-react-hooks"
    ],
    "jsRules": {},
    "rules": {
      "variable-name": false,
      "import-name": false,
      "react-hooks-nesting": "error",
      "react-hooks/exhaustive-deps": "error",
      "max-line-length": [true, 150],
    },
    "rulesDirectory": []
}
